{
    "training": {
        "batch_size": 32,
        "learning_rate": 1e-4,
        "epochs": 100,
        "num_workers": 4,
        "kl_weight": 0.001,
        "perceptual_weight": 0.1,
        "adv_weight": 0.01,
        "log_interval": 10,
        "save_interval": 5,
        "val_interval": 5,
        "amp": true
    },
    "model": {
        "autoencoder": {
            "spatial_dims": 2,
            "in_channels": 1,
            "out_channels": 1,
            "latent_channels": 4,
            "num_channels": [64, 128, 256],
            "num_res_blocks": [2, 2, 2],
            "norm_num_groups": 32,
            "norm_eps": 1e-6,
            "attention_levels": [false, false, false],
            "with_encoder_nonlocal_attn": false,
            "with_decoder_nonlocal_attn": false,
            "use_checkpointing": false,
            "use_convtranspose": false,
            "norm_float16": true,
            "num_splits": 1,
            "dim_split": 1
        },
        "discriminator": {
            "spatial_dims": 2,
            "num_layers_d": 3,
            "channels": 32,
            "in_channels": 1,
            "out_channels": 1,
            "norm": "INSTANCE"
        }
    },
    "data": {
        "train_transform": {
            "resize": [256, 256],
            "random_crop_scale": [0.8, 1.0],
            "random_flip_prob": 0.5,
            "random_rotation_angle": 10,
            "brightness_adjustment": 0.2,
            "contrast_adjustment": 0.2,
            "speckle_noise_std": 0.1,
            "normalize_mean": [0.2100],
            "normalize_std": [0.0300]
        },
        "val_transform": {
            "resize": [256, 256],
            "normalize_mean": [0.2100],
            "normalize_std": [0.0300]
        }
    }
}