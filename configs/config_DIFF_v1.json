            "in_channels": 8,
            "out_channels": 125,
            "latent_channels": "@latent_channels",
            "num_channels": [32, 64, 128],
            "num_res_blocks": [1, 2, 2],
            "norm_num_groups": 32,
            "norm_eps": 1e-06,
            "attention_levels": [false, false, false],
