            "attention_levels": [false, false, true, true],
            "num_head_channels": [0, 0, 32, 32],
            "num_res_blocks": 2,
            "use_flash_attention": true,
            "conditioning_embedding_in_channels": 8,
            "conditioning_embedding_num_channels": [8, 32, 64]
